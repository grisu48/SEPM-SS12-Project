<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='org_smartsnip'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
	

  <!-- Other module inherits                                      -->
  
  <!-- Include external JavaScript packages.                      -->
  <!-- the google code prettifyer                                 -->
  <script src='../googleCodePrettify/prettify.js'/>
  <!-- Additional language handlers                               -->
  <script src='../googleCodePrettify/lang-tex.js'/>
  <script src='../googleCodePrettify/lang-sql.js'/>
  <script src='../googleCodePrettify/lang-css.js'/>
  <script src='../googleCodePrettify/lang-vb.js'/>
  <script src='../googleCodePrettify/lang-ml.js'/>
  <script src='../googleCodePrettify/lang-vhdl.js'/>
  <script src='../googleCodePrettify/lang-hs.js'/>
  <script src='../googleCodePrettify/lang-xq.js'/>
  <!-- Optional language handlers                                 -->
  <!-- <script src='../googleCodePrettify/lang-apollo.js'/>       -->
  <!-- <script src='../googleCodePrettify/lang-clj.js'/>          -->
  <!-- <script src='../googleCodePrettify/lang-go.js'/>           -->
  <!-- <script src='../googleCodePrettify/lang-lisp.js'/>         -->
  <!-- <script src='../googleCodePrettify/lang-lua.js'/>          -->
  <!-- <script src='../googleCodePrettify/lang-n.js'/>            -->
  <!-- <script src='../googleCodePrettify/lang-proto.js'/>        -->
  <!-- <script src='../googleCodePrettify/lang-scala.js'/>        -->
  <!-- <script src='../googleCodePrettify/lang-wiki.js'/>         -->
  <!-- <script src='../googleCodePrettify/lang-yaml.js'/>         -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.smartsnip.client.Control'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
